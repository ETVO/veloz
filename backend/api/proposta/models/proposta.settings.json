{
  "kind": "collectionType",
  "collectionName": "propostas",
  "info": {
    "name": "proposta",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "regimeCasamento": {
      "type": "enumeration",
      "enum": [
        "Total",
        "Parcial",
        "Separado",
        "Participacao"
      ],
      "required": false
    },
    "estadoCivil": {
      "type": "enumeration",
      "enum": [
        "Solteiro",
        "Casado",
        "Viuvo",
        "Separado",
        "Divorciado"
      ],
      "required": true
    },
    "endereco": {
      "type": "string",
      "required": true
    },
    "bairro": {
      "type": "string",
      "required": true
    },
    "cep": {
      "type": "string",
      "required": true,
      "regex": "[0-9]{5}-[0-9]{3}"
    },
    "cidade": {
      "type": "string",
      "required": true
    },
    "telefone": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "infoConjuge": {
      "type": "component",
      "repeatable": false,
      "component": "proposta.conjuge"
    },
    "infoComprador": {
      "type": "component",
      "repeatable": false,
      "component": "proposta.conjuge"
    },
    "empreendimento": {
      "via": "propostas",
      "model": "empreendimento"
    },
    "autenticada": {
      "type": "boolean"
    },
    "unidades": {
      "type": "json"
    }
  }
}
